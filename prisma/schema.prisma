generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model users {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  username String   @unique
  password String
}



